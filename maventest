node{
   stage('Code Checkout') {
            steps {
                checkout([
                    $class: 'GitSCM', 
                    branches: [[name: '*/main']], 
                    userRemoteConfigs: [[url: 'https://github.com/spring-projects/spring-petclinic.git']]
                ])
            }
    }

   stage('Compile-Package'){
     def mvnHome = tool name: '', type: 'maven'
     sh "${mvnHome}/bin/mvn package"   
   }
}
